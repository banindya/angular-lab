{
	"compileOnSave": false,
	"compilerOptions": {
		"baseUrl": "src",
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"downlevelIteration": true,
		"declaration": false,
		"module": "es2015",
		"moduleResolution": "node",
		"target": "es5",
		"importHelpers": true,
		"noUnusedLocals": true,
		"noUnusedParameters": false,
		"strict": true,
		"sourceMap": true,
		"outDir": "./dist/out-tsc",
		"typeRoots": [
			"node_modules/@types"
		],
		"lib": [
			"es2017",
			"dom"
		]
	}
}
