{
    "compileOnSave": false,
    "compilerOptions": {
        "baseUrl": "src",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "downlevelIteration": true,
        "declaration": false,
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es5",
        "importHelpers": true,
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "strict": true,
        "sourceMap": true,
        "skipLibCheck": true,
        "outDir": "./dist/out-tsc",
        "typeRoots": [
            "node_modules/@types"
        ],
        "paths": {
            "secrets": [
                "../.secrets.js"
            ]
        },
        "lib": [
            "es2017",
            "dom"
        ]
    }
}
